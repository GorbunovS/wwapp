{
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "types": ["jest"],
    "target": "ESNext",
    "module": "ESNext",
    "strict": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src", "**/*.test.ts", "**/*.test.tsx"],
  "exclude": ["node_modules"]
  // "references": [{ "path": "./tsconfig.app.json" }, { "path": "./tsconfig.node.json" }]
}
